{
  "scopeName": "source.m",
  "name": "M DGFiP",
  "fileTypes": [
    "m"
  ],
  "patterns": [
    {
      "match": "\\bvariable\\s+(saisie|calculee)\\b",
      "name": "entity.name.type.m"
    },
    {
      "match": "\\bdomaine\\s+(regle|verif)\\b",
      "name": "entity.name.type.m"
    },
    {
      "match": "\\b(regle|verif|cible)\\b",
      "name": "entity.name.type.m"
    },
    {
      "match": "\\bcalculer\\s+(domaine|enchaineur|cible)\\b",
      "name": "keyword.other.m"
    },
    {
      "match": "\\bverifier\\s+domaine\\b",
      "name": "keyword.other.m"
    },
    {
      "match": "\\:|\\(|\\)|\\,|\\;",
      "name": "keyword.other.m"
    },
    {
      "match": "\\bvariable\\s+temporaire\\b",
      "name": "keyword.other.m"
    },
    {
      "match": "\\b(application|enchaineur|tableau|attribut)\\b",
      "name": "keyword.other.m"
    },
    {
      "match": "\\b(specialise|autorise|par_defaut|calculable|verifiable)\\b",
      "name": "keyword.other.m"
    },
    {
      "match": "\\b(saisie|calculee|alias|base|restituee|type|fonction|const|sortie|condition)\\b",
      "name": "keyword.other.m"
    },
    {
      "match": "\\b(erreur|afficher_erreur|leve_erreur|nettoie_erreurs)\\b",
      "name": "keyword.other.m"
    },
    {
      "match": "\\b(si|alors|sinon|sinon_si|finsi|pour|un|iterer|avec|restaurer|apres|categorie)\\b",
      "name": "keyword.control.m"
    },
    {
      "match": "\\b(erreur|afficher_erreur|leve_erreur|nettoie_erreurs)\\b",
      "name": "keyword.control.m"
    },
    {
      "match": ":\\s*variable",
      "name": "keyword.control.m"
    },
    {
      "begin": "\\\"",
      "end": "\\\"",
      "name": "string.quoted.double.m"
    },
    {
      "match": "^\\s*([A-Z_a-z_0-9]+)[A-Z_a-z]([A-Z_a-z_0-9]*)\\s*\\=aaaaaaaaaaaaaaaaaaaaaaaaaa",
      "comment": "deactivated because of conflicts with equality test",
      "name": "entity.name.type.m"
    },
    {
      "match": "^\\s*([A-Z_a-z_0-9]*)[A-Z]([A-Z_a-z_0-9]*)\\s*\\:",
      "name": "entity.name.type.m"
    },
    {
      "match": "\\b([0-9]+)(\\(\\.[0-9]+\\))?\\b",
      "name": "constant.numeric.m"
    },
    {
      "match": "\\bindefini\\b",
      "name": "constant.numeric.m"
    },
    {
      "match": "\\b(somme|abs|min|max|positif|positif_ou_nul|null|arr|inf|supzero|present)\\b",
      "name": "entity.name.function.m"
    },
    {
      "match": "\\b(multimax|modulo)\\b",
      "name": "entity.name.function.m"
    },
    {
      "match": "\\b(nb_categorie|nb_erreur|numero_compl|numero_verif|taille)\\b",
      "name": "entity.name.function.m"
    },
    {
      "name": "comment.line.m",
      "begin": "#",
      "end": "$"
    },
    {
      "name": "comment.block.m",
      "begin": "#\\{",
      "end": "\\}#"
    },
    {
      "match": "\\b(\\+|\\-|\\*|\\|\\=|!\\=|\\>|\\<|\\>\\=|\\<\\=|et|ou|non|dans)\\b",
      "name": "keyword.operator.m"
    },
    {
      "match": "\\b([A-Z0-9]*)[A-Z]([A-Z0-9_]*)\\b",
      "name": "entity.name.variable.m"
    }
  ]
}